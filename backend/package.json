    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.5",
    "nodemon": "^3.0.2",
    "prisma": "^6.16.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
